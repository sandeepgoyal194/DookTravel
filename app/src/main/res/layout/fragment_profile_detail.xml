                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_gravity="center"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_marginTop="15dp"
                    android:layout_marginBottom="15dp"
                    android:background="@color/white"
                    android:orientation="vertical">


                    <com.softmine.dooktravel.util.CustomTextView
                        custom:typeface="proxima-nova-light.otf"
                        android:layout_marginLeft="5dp"
                        android:id="@+id/tvCategory"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/name"

                        android:textColor="@color/black"
                        android:textSize="14sp" />
                    <com.softmine.dooktravel.util.CustomEditText
                        custom:typeface="proxima-nova-semibold.otf"
                            android:id="@+id/edFirstname"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:lines="1"
                            android:theme="@style/EditText.custom_details"
                            style="@style/EditText.custom_details"
                            android:focusable="false"
                            android:textColor="@color/background_blue"
                            android:textSize="14sp" />

                    <android.support.design.widget.TextInputLayout
                        android:id="@+id/ed_layout_MiddleName"
                        android:layout_width="match_parent"
                        android:textColorHint="@color/hint"
                        android:visibility="gone"
                        android:layout_height="wrap_content">

                        <com.softmine.dooktravel.util.CustomEditText
                            custom:typeface="proxima-nova-semibold.otf"
                            android:id="@+id/edMiddleName"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:hint="@string/middle_name"
                            android:lines="1"
                            android:visibility="gone"
                            android:textColor="@color/pilot"
                            android:focusable="false"
                            android:textSize="14sp" />
                    </android.support.design.widget.TextInputLayout>

                    <android.support.design.widget.TextInputLayout
                        android:id="@+id/ed_layout_LastName"
                        android:layout_width="match_parent"
                        android:visibility="gone"

