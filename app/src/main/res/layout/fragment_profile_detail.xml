                            android:id="@+id/edlastName"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:hint="@string/last_name"
                            android:focusable="false"
                            android:visibility="gone"
