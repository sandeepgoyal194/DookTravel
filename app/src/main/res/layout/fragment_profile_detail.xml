    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:custom="http://schemas.android.com/apk/res-auto/com.softmine.dooktravel.util"

        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        >
        <ImageView
            android:id="@+id/imgProfile"
            android:scaleType="fitXY"
            android:background="@color/grey"
            android:layout_width="match_parent"
            android:layout_height="150dp"
            />
        <!-- TODO: Update blank fragment layout -->

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/imgProfile"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true">

            <LinearLayout

                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">




                <LinearLayout
                    android:id="@+id/ll"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_gravity="center"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:layout_marginTop="15dp"
                    android:layout_marginBottom="15dp"
                    android:background="@drawable/back_basic_detail"
                    android:orientation="vertical">


                    <com.softmine.dooktravel.util.CustomTextView
                        custom:typeface="proxima-nova-light.otf"
                        android:layout_marginLeft="5dp"
                        android:id="@+id/tvCategory"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/name"
                        android:textColor="@color/black"
                        android:textSize="14sp" />
                        <EditText
                            android:id="@+id/edFirstname"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:lines="1"
                            android:theme="@style/EditText.profile"
                            style="@style/EditText.profile"
                            android:focusable="false"
                            android:textColor="@color/pilot"
                            android:textSize="14sp" />

                    <android.support.design.widget.TextInputLayout
                        android:id="@+id/ed_layout_MiddleName"
                        android:layout_width="match_parent"
                        android:textColorHint="@color/hint"
                        android:visibility="gone"
                        android:layout_height="wrap_content">

                        <EditText
                            android:id="@+id/edMiddleName"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:hint="@string/middle_name"
                            android:lines="1"
                            android:visibility="gone"
                            android:textColor="@color/pilot"
                            android:focusable="false"
                            android:textSize="14sp" />
                    </android.support.design.widget.TextInputLayout>

                    <android.support.design.widget.TextInputLayout
                        android:id="@+id/ed_layout_LastName"
                        android:layout_width="match_parent"
                        android:visibility="gone"

                        android:textColorHint="@color/hint"
                        android:layout_height="wrap_content">


                        <EditText
                            android:id="@+id/edlastName"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:hint="@string/last_name"
                            android:focusable="false"
                            android:visibility="gone"
                            android:lines="1"
                            android:textColor="@color/pilot"

                            android:textSize="14sp" />
                    </android.support.design.widget.TextInputLayout>




                    <com.softmine.dooktravel.util.CustomTextView
                        custom:typeface="proxima-nova-light.otf"
                        android:layout_marginTop="2dp"
                        android:layout_marginLeft="5dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/address"
                        android:textColor="@color/black"
                        android:textSize="14sp" />
                        <EditText
                            android:id="@+id/edAddress"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:lines="1"
                            android:focusable="false"
                            android:theme="@style/EditText.profile"
                            style="@style/EditText.profile"
                            android:textColor="@color/pilot"
                            android:textSize="14sp" />


                    <com.softmine.dooktravel.util.CustomTextView
                        custom:typeface="proxima-nova-light.otf"
                        android:layout_marginTop="2dp"
                        android:layout_marginLeft="5dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/email"
                        android:textColor="@color/black"
                        android:textSize="14sp" />
                        <EditText
                            android:id="@+id/edEmail"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textColor="@color/pilot"
                            android:lines="1"
                            android:focusable="false"
                            android:theme="@style/EditText.profile"
                            style="@style/EditText.profile"
                            android:textSize="14sp" />






                    <com.softmine.dooktravel.util.CustomTextView
                        custom:typeface="proxima-nova-light.otf"
                        android:layout_marginTop="2dp"
                        android:layout_marginLeft="5dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/contact"
                        android:textColor="@color/black"
                        android:textSize="14sp" />

                        <EditText
                            android:id="@+id/edContact"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:focusable="false"
                            android:theme="@style/EditText.profile"
                            style="@style/EditText.profile"
                            android:lines="1"
                            android:textColor="@color/pilot"
                            android:textSize="14sp" />



                    <com.softmine.dooktravel.util.CustomTextView
                        custom:typeface="proxima-nova-light.otf"
                        android:layout_marginTop="2dp"
                        android:layout_marginLeft="5dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/skype"
                        android:textColor="@color/black"
                        android:textSize="14sp" />
                        <EditText
                            android:id="@+id/edSkype"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:focusable="false"
                            android:theme="@style/EditText.profile"
                            style="@style/EditText.profile"
                            android:lines="1"
                            android:textColor="@color/pilot"

                            android:textSize="14sp" />
                    <com.softmine.dooktravel.util.CustomTextView
                        custom:typeface="proxima-nova-light.otf"
                        android:layout_marginTop="2dp"
                        android:layout_marginLeft="5dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/description"
                        android:textColor="@color/black"
                        android:textSize="14sp" />




                        <EditText
                            android:id="@+id/edDescription"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:focusable="false"
                            android:lines="1"
                            android:textColor="@color/pilot"
                            android:theme="@style/EditText.profile"
                            style="@style/EditText.profile"
                            android:textSize="14sp" />

                    <com.softmine.dooktravel.util.CustomTextView
                        custom:typeface="proxima-nova-light.otf"
                        android:layout_marginTop="2dp"
                        android:layout_marginLeft="5dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/gender"
                        android:textColor="@color/black"
                        android:textSize="14sp" />
                        <EditText
                            android:id="@+id/edGender"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textColor="@color/pilot"
                            android:lines="1"
                            android:focusable="false"
                            android:theme="@style/EditText.profile"
                            style="@style/EditText.profile"
                            android:textSize="14sp" />


                    <com.softmine.dooktravel.util.CustomTextView
                        custom:typeface="proxima-nova-light.otf"
                        android:layout_marginTop="2dp"
                        android:layout_marginLeft="5dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/date_of_birth"
                        android:textColor="@color/black"
                        android:textSize="14sp" />

                        <EditText
                            android:id="@+id/edDob"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textColor="@color/pilot"
                            android:lines="1"
                            android:focusable="false"
                            android:theme="@style/EditText.profile"
                            style="@style/EditText.profile"
                            android:textSize="14sp" />




                    <com.softmine.dooktravel.util.CustomTextView
                        custom:typeface="proxima-nova-light.otf"
                        android:layout_marginTop="2dp"
                        android:layout_marginLeft="5dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/marital_status"
                        android:textColor="@color/black"
                        android:textSize="14sp" />

                        <EditText
                            android:id="@+id/edMatiral_status"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textColor="@color/pilot"
                            android:lines="1"
                            android:focusable="false"
                            android:theme="@style/EditText.profile"
                            style="@style/EditText.profile"
                            android:textSize="14sp" />



                    <com.softmine.dooktravel.util.CustomTextView
                        custom:typeface="proxima-nova-light.otf"
                        android:layout_marginTop="2dp"
                        android:layout_marginLeft="5dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/organization"
                        android:textColor="@color/black"
                        android:textSize="14sp" />

                        <EditText
                            android:id="@+id/edOrganization"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:lines="1"
                            android:theme="@style/EditText.profile"
                            style="@style/EditText.profile"
                            android:focusable="false"
                            android:singleLine="true"
                            android:textColor="@color/pilot"
                            android:textSize="14sp" />

                    <com.softmine.dooktravel.util.CustomTextView
                        custom:typeface="proxima-nova-light.otf"
                        android:layout_marginTop="2dp"
                        android:layout_marginLeft="5dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/designation"
                        android:textColor="@color/black"
                        android:textSize="14sp" />

                        <EditText
                            android:id="@+id/edDesignation"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:singleLine="true"
                            android:lines="1"
                            android:theme="@style/EditText.profile"
                            style="@style/EditText.profile"

                            android:focusable="false"
                            android:textColor="@color/pilot"
                            android:textSize="14sp" />

                    <com.softmine.dooktravel.util.CustomTextView
                        custom:typeface="proxima-nova-light.otf"
                        android:layout_marginTop="2dp"
                        android:layout_marginLeft="5dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/working_since"
                        android:textColor="@color/black"
                        android:textSize="14sp" />

                        <EditText
                            android:id="@+id/edWorkingSince"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textColor="@color/pilot"
                            android:lines="1"
                            android:focusable="false"
                            android:theme="@style/EditText.profile"
                            style="@style/EditText.profile"
                            android:textSize="14sp" />

                    <com.softmine.dooktravel.util.CustomTextView
                        custom:typeface="proxima-nova-light.otf"
                        android:layout_marginTop="2dp"
                        android:layout_marginLeft="5dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/category"
                        android:textColor="@color/black"
                        android:textSize="14sp" />

                        <EditText
                            android:id="@+id/edCategory"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textColor="@color/pilot"
                            android:lines="1"
                            android:focusable="false"
                            android:theme="@style/EditText.profile"
                            style="@style/EditText.profile"
                            android:textSize="14sp" />


                    <com.softmine.dooktravel.util.CustomTextView
                        custom:typeface="proxima-nova-light.otf"
                        android:layout_marginTop="2dp"
                        android:layout_marginLeft="5dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/country"
                        android:textColor="@color/black"
                        android:textSize="14sp" />
                        <EditText
                            android:id="@+id/edCountry"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textColor="@color/pilot"
                            android:lines="1"
                            android:focusable="false"
                            android:theme="@style/EditText.profile"
                            style="@style/EditText.profile"
                            android:textSize="14sp" />






                    <com.softmine.dooktravel.util.CustomTextView
                        custom:typeface="proxima-nova-light.otf"
                        android:layout_marginTop="2dp"
                        android:layout_marginLeft="5dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/province"
                        android:textColor="@color/black"
                        android:textSize="14sp" />

                        <EditText
                            android:id="@+id/edProvince"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textColor="@color/pilot"
                            android:lines="1"
                            android:focusable="false"
                            android:theme="@style/EditText.profile"
                            style="@style/EditText.profile"
                            android:textSize="14sp" />


                    <com.softmine.dooktravel.util.CustomTextView
                        custom:typeface="proxima-nova-light.otf"
                        android:layout_marginTop="2dp"
                        android:layout_marginLeft="5dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/city"
                        android:textColor="@color/black"
                        android:textSize="14sp" />

                        <EditText
                            android:id="@+id/edCity"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"

                            android:textColor="@color/pilot"
                            android:lines="1"
                            android:focusable="false"
                            android:theme="@style/EditText.profile"
                            style="@style/EditText.profile"
                            android:textSize="14sp" />


                    <com.softmine.dooktravel.util.CustomTextView
                        custom:typeface="proxima-nova-light.otf"
                        android:layout_marginTop="2dp"
                        android:layout_marginLeft="5dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/zip"
                        android:textColor="@color/black"
                        android:textSize="14sp" />

                        <EditText
                            android:id="@+id/edZipCode"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textColor="@color/pilot"
                            android:lines="1"
                            android:focusable="false"
                            android:theme="@style/EditText.profile"
                            style="@style/EditText.profile"
                            android:textSize="14sp" />



                </LinearLayout>


            </LinearLayout>
        </ScrollView>

    </RelativeLayout>
