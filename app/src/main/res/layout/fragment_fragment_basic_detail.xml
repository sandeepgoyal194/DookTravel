                                    android:layout_marginRight="4dp" />
                            </LinearLayout>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical">

                                <TextView

                                    android:id="@+id/tvProvince"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="@string/province"
                                    android:textColor="@color/black"

                                    android:textSize="14sp"
                                    android:visibility="gone" />

                                <Spinner
                                    android:paddingLeft="36dp"
                                    android:gravity="start"
                                    android:layout_gravity="start"
                                    android:id="@+id/spinner_province"
                                    style="@style/SpinnerTheme_state"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="4dp"
                                    android:layout_marginRight="4dp" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/tvCity"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="@string/city"
                                    android:textColor="@color/black"

                                    android:textSize="14sp"
                                    android:visibility="gone" />

                                <Spinner
                                    android:paddingLeft="36dp"
                                    android:gravity="start"
                                    android:layout_gravity="start"
                                    android:id="@+id/spinner_city"
                                    style="@style/SpinnerTheme_city"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="4dp"
                                    android:layout_marginRight="4dp" />
                            </LinearLayout>





                            <LinearLayout
                                android:layout_marginTop="-10dp"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/tvPostalCode"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:lines="1"
                                    android:text="@string/zip"
                                    android:textColor="@color/black"
                                    android:textSize="14sp"
                                    android:visibility="gone" />

                                <com.softmine.dooktravel.util.CustomEditText
                                    custom:typeface="proxima-nova-semibold.otf"
                                    android:id="@+id/et_Postal"
                                    style="@style/EditText.custom_details"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:drawableLeft="@drawable/zip"
                                    android:drawablePadding="20dp"
                                    android:hint="@string/zip"
                                    android:inputType="number"
                                    android:lines="1"

                                    android:maxLength="6"
                                    android:paddingLeft="10dp"
                                    android:singleLine="true"
                                    android:textColor="@color/edittext_color"
                                    android:textColorHint="@color/hint"
                                    android:textSize="14sp"
                                    android:theme="@style/EditText.custom_details" />
                            </LinearLayout>

                            <!-- <android.support.design.widget.TextInputLayout
                                 android:id="@+id/ed_layout_address"
                                 android:layout_width="match_parent"
                                 android:layout_height="wrap_content"
                                 android:layout_marginTop="10dp"
                                 android:lines="1"
                                 >-->

                            <com.softmine.dooktravel.util.CustomEditText
                                custom:typeface="proxima-nova-semibold.otf"
                                android:id="@+id/edAddress"
                                style="@style/EditText.custom_details"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:drawableLeft="@drawable/street"
                                android:drawablePadding="20dp"
                                android:hint="@string/street"
                                android:lines="1"
                                android:paddingLeft="10dp"
                                android:singleLine="true"
                                android:textColor="@color/edittext_color"
                                android:textColorHint="@color/hint"
                                android:textSize="14sp"
                                android:theme="@style/EditText.custom_details" />
                            <!--   </android.support.design.widget.TextInputLayout>
            -->
                            <LinearLayout

                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical">


                                <com.softmine.dooktravel.util.CustomTextView
                                    custom:typeface="ProximaNova-Bold.otf"
                                    android:id="@+id/tvAboutUs"
                                    android:layout_width="match_parent"
                                    android:layout_height="30dp"
                                    android:layout_gravity="center_horizontal"
                                    android:background="@color/title_bar_color"
                                    android:gravity="center"
                                    android:paddingLeft="20dp"
                                    android:text="@string/about_me"
                                    android:textColor="@color/title_text_color"
                                    android:textSize="16sp" />

                                <com.softmine.dooktravel.util.CustomEditText
                                    custom:typeface="proxima-nova-semibold.otf"
                                    android:id="@+id/edAboutMe"

                                    style="@style/EditText.custom_details"
                                    android:layout_width="match_parent"
                                    android:layout_height="100dp"
                                    android:layout_gravity="top"
                                    android:layout_marginLeft="5dp"
                                    android:layout_marginRight="15dp"
                                    android:layout_marginTop="15dp"
                                    android:background="@drawable/view_back_border"
                                    android:drawablePadding="20dp"
                                    android:gravity="top"
                                    android:hint="@string/about_me"
                                    android:maxLength="250"
                                    android:paddingLeft="10dp"
                                    android:paddingTop="10dp"
                                    android:textColor="@color/edittext_color"
                                    android:textColorHint="@color/hint"
                                    android:textSize="14sp"
                                    android:theme="@style/EditText.custom_details" />

                                <com.softmine.dooktravel.util.CustomTextView
                                    custom:typeface="ProximaNova-Bold.otf"
                                    android:id="@+id/tvTextCount"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="right"
                                    android:layout_marginBottom="100dp"
                                    android:layout_marginLeft="15dp"
                                    android:layout_marginRight="15dp"
                                    android:layout_marginTop="2dp"
                                    android:hint="@string/tv_no_of_char"
                                    android:textColor="@color/background_blue" />

                            </LinearLayout>
                        </LinearLayout>

                    </LinearLayout>
                </RelativeLayout>
            </ScrollView>
        </RelativeLayout>
    </RelativeLayout>
    </android.support.v4.widget.NestedScrollView>

    <RelativeLayout

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerVertical="true"
        >

        <com.softmine.dooktravel.util.CustomButton
            custom:typeface="proxima-nova-semibold.otf"
            android:id="@+id/btnNext"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"

            android:background="@drawable/button_round"
            android:text="@string/submit"
            android:textColor="@color/white"
            android:textSize="16sp" />
    </RelativeLayout>
</RelativeLayout>
</android.support.design.widget.CoordinatorLayout>